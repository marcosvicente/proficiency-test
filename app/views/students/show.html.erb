<div id="all">
    <p id="notice"><%= notice %></p>

    <p>
        <strong>Name:</strong>
        <%= @student.name %>
    </p>

    <p>
        <strong>Register number:</strong>
        <%= @student.register_number %>
    </p>

    <p>
        <strong>Status:</strong>
        <%= @student.status %>
    </p>
    <div class="link-edition">
    <%= link_to 'Edit', edit_student_path(@student) %> 
    <%= link_to 'Back', students_path %>
    </div>
</div>
